<p id="notice"><%= notice %></p>

<h1>Archivos</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>  
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @reports.each do |report| %>
      <tr>
        <td><%= report.title %></td>
        <td><%= link_to 'Ver', report %></td>
        <td><%= link_to 'Editar', edit_report_path(report) %></td>
        <td><%= link_to 'Eliminar', report, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nuevo Archivo', new_report_path %>
